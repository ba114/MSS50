<!--Timing-->

	<table type="3D" name="Ignition Timing - Idle" category="Timing" storagetype="int16" endian="big"  userlevel="1" sizex="6" sizey="2">
		<scaling units="deg" expression="x*0.1" to_byte="x/0.1" format="0.0" fineincrement="0.5" coarseincrement="1" />
			<table type="X Axis" name="RPM" storagetype="uint16" endian="big">
				<scaling units="RPM" expression="x" to_byte="x" format="0" fineincrement="50" coarseincrement="100" />
			</table>
			<table type="Y Axis" name="Load" storagetype="uint16" endian="big" logparam="P8" >
				<scaling units="ms" expression="x*0.001" to_byte="x/0.001" format="0.00" fineincrement="1" coarseincrement="10" />
			</table>
		<description>This ignition timing advance map is used during engine idle mode.</description>
	</table>
	
	<table type="3D" name="Ignition Timing - Part Throttle" category="Timing" storagetype="int16" endian="big" sizex="18" sizey="12" userlevel="1" >
		<scaling units="deg" expression="x*0.1" to_byte="x/0.1" format="0.00" fineincrement=".01" coarseincrement="1" />
			<table type="X Axis" name="RPM" storagetype="uint16" endian="big">
				<scaling units="RPM" expression="x" to_byte="x" format="#" fineincrement="1" coarseincrement="10" />
			</table>
			<table type="Y Axis" name="Load" storagetype="uint16" endian="big">
				<scaling units="ms" expression="x*0.001" to_byte="x/0.001" format="0.00" fineincrement="1" coaeincrement="10" />
			</table>
		<description>(KF_TZ_GRUND) This is the main ignition timing advance map</description>
	</table>

	<table type="3D" name="Ignition Timing - Part Throttle - Knock Limited" category="Timing" storagetype="int16" endian="big" sizex="18" sizey="12" userlevel="1" >
		<scaling units="deg" expression="x*0.1" to_byte="x/0.1" format="0.00" fineincrement=".01" coarseincrement="1" />
			<table type="X Axis" name="RPM" storagetype="uint16" endian="big">
				<scaling units="RPM" expression="x" to_byte="x" format="#" fineincrement="1" coarseincrement="10" />
			</table>
			<table type="Y Axis" name="Load" storagetype="uint16" endian="big">
				<scaling units="ms" expression="x*0.001" to_byte="x/0.001" format="0.00" fineincrement="1" coaeincrement="10" />
			</table>
		<description>This is the ignition timing map when experiencing knock</description>
	</table>	
	
	<table type="3D" name="Ignition Timing - WOT" category="Timing" storagetype="int16" endian="big"  userlevel="1" sizex="18" sizey="3">
		<scaling units="deg" expression="x*0.1" to_byte="x/0.1" format="0.0" fineincrement="0.5" coarseincrement="1" />
			<table type="X Axis" name="RPM" storagetype="uint16" endian="big">
				<scaling units="RPM" expression="x" to_byte="x" format="0" fineincrement="50" coarseincrement="100" />
			</table>
			<table type="Y Axis" name="Load" storagetype="uint16" endian="big" logparam="P8" >
				<scaling units="ms" expression="x*0.001" to_byte="x/0.001" format="0.00" fineincrement="1" coarseincrement="10" />
			</table>
		<description>This ignition timing advance map is used during WOT.</description>
	</table>


