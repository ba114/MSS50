<!--Alpha-N-->

	<table type="3D" name="Calculated Load" category="Alpha-N" storagetype="uint16" endian="big" sizex="7" sizey="6" userlevel="1" >
		<scaling units="ms" expression="x*0.001" to_byte="x/0.001" format="0.00" fineincrement="1" coarseincrement="10" />
			<table type="X Axis" name="TPS" storagetype="uint16" endian="big">
				<scaling units="%" expression="x*(100/256)" to_byte="x/(100/256)" format="0.00" fineincrement="1" coaeincrement="10" />
			</table>
			<table type="Y Axis" name="Engine Speed" storagetype="uint16" endian="big">
				<scaling units="RPM" expression="x" to_byte="x" format="0" fineincrement="10" coarseincrement="50" />
			</table>
		<description>(KF_RF_N_AQ_REL) This table calculates load based on RPM and TPS position</description>
	</table> 


<!--knock-->
	<table type="3D" name="Knock Detection Threshold - Cylinder 1" category="Knock" storagetype="uint16" endian="big" sizex="16" sizey="3" userlevel="1" >
		<scaling units="%" expression="x*(1/1024)" to_byte="x/(1/1024)" format="0.00" fineincrement="0.1" coarseincrement="1" />
			<table type="X Axis" name="RPM" storagetype="uint16" endian="big">
				<scaling units="RPM" expression="x" to_byte="x" format="0" fineincrement="1" coarseincrement="10" />
			</table>
			<table type="Y Axis" name="Load" storagetype="uint16" endian="big">
				<scaling units="ms" expression="x*0.001" to_byte="x/0.001" format="0.00" fineincrement="1" coaeincrement="10" />
			</table>
		<description>(KF_KD_KFACT_1) The amount of noise the DME will accept before determining knock</description>
