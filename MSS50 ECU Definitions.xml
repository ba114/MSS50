<!--Scalars-->
 
	<table type="2D" name="ECT Sensor Scalar" category="Sensors Scaling" storagetype="uint16" endian="little" sizex="11" userlevel="1">
		<scaling units="°C" expression="(x*0.00390625)-48" to_byte="(x+48)/0.00390625" format="0.00" fineincrement="2" coarseincrement="10"/>
			<table type="Y Axis" name="Voltage" storagetype="uint16" endian="big">
				<scaling units="Volts" expression="x*0.00488" to_byte="x/0.00488" format="0.00" fineincrement=".01" coarseincrement=".1"/>
			</table>
